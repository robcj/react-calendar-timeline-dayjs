    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.12.0",
    "@testing-library/jest-dom": "^6.5.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.2.1",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "cheerio": "^1.0.0-rc.12",
    "cross-env": "^5.1.4",
    "css-loader": "^7.1.2",
    "dayjs": "^1.11.13",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.8",
    "eslint": "^9.12.0",
