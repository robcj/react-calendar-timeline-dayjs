    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "jsdom": "^25.0.1",
    "prettier": "^3.3.3",
    "prettier-eslint-cli": "^8.0.1",
    "prop-types": "^15.8.1",
    "randomcolor": "^0.5.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^5.3.4",
    "react-testing-library": "^6.0.3",
